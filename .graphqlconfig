  "schemaPath": "schema.graphql",
  "extensions": {
    "endpoints": {
      "development": {
        "url": "http://localhost:${PORT}${GQL_ENDPOINT}",
        "headers": {
          "Authorization": "Bearer ${AUTH_TOKEN}"
        },
        "subscription": {
          "url": "ws://localhost:${PORT}${GQL_SUBSCRIPTIONENDPOINT}",
          "connectionParams": {
            "Token": "${AUTH_TOKEN}"
          }
        }
      }
    }
  }
}