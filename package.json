        "graphql-tag": "^2.11.0",
        "graphql-voyager": "^1.0.0-rc.28",
        "ioredis": "^4.16.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.1",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.27",
