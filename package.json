{
    "name": "@via-profit-services/core",
    "support": "https://via-profit.ru",
    "version": "2.1.1",
    "description": "Via Profit Core service",
    "main": "dist/index.js",
    "keywords": [
        "graphql",
        "via-profit",
        "express"
    ],
    "scripts": {
        "dist": "yarn lint && yarn test && yarn build:dist",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "debug": "cross-env DEBUG=true yarn build:dev --watch",
        "debug-plain": "yarn node --inspect-brk=9229 ./build/index.js",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "start": "yarn build:dev --watch",
        "lint": "yarn eslint --ext .ts ./src/ --fix .",
        "package": "yarn pack -o package-%v.tgz"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/core.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vasily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "devDependencies": {
        "@graphql-tools/stitch": "^8.4.2",
        "@types/bluebird": "^3.5.29",
        "@types/content-type": "^1.1.3",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.9",
        "@types/node": "^14.14.10",
        "@types/nodemon": "^1.19.0",
        "@types/validator": "^12.0.1",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-merge": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "5.1.0",
        "@typescript-eslint/parser": "5.1.0",
        "@via-profit-services/eslint-config": "^0.1.19",
        "cors": "^2.8.5",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-import-resolver-webpack": "0.13.2",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-standard": "^5.0.0",
        "express": "^4.17.1",
        "graphql": "16.0.1",
        "nodemon": "^2.0.6",
        "nodemon-webpack-plugin": "^4.3.2",
        "prettier": "^2.4.0",
        "supertest": "^4.0.2",
        "supports-color": "^8.0.0",
        "terser-webpack-plugin": "^5.0.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.4.3",
        "webpack": "^5.65.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-merge": "^5.8.0"
    },
    "peerDependencies": {
        "express": "^4.x",
        "graphql": "^15.x || ^16.x"
    },
    "optionalDependencies": {
        "bufferutil": "^4.x",
        "utf-8-validate": "^5.x"
    },
    "dependencies": {
        "raw-body": "^2.x"
    }
