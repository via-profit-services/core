{
    "name": "@via-profit-services/core",
    "private": true,
    "support": "https://via-profit.ru",
    "version": "0.13.0",
    "description": "Via Profit Core service",
    "main": "dist/index.js",
    "scripts": {
        "dist": "yarn lint && yarn test && yarn build:dist",
        "start": "yarn start:dev",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
        "build:dist": "webpack --config ./webpack/webpack.config.prod.js",
        "start:dev": "yarn build:dev --watch",
        "start:dist": "yarn dist && node ./dist/playground.js",
        "test": "jest",
        "lint": "eslint --ext .ts ./src/ --fix .",
        "knex:migrate:make": "knex migrate:make --knexfile src/utils/knexfile.ts",
        "knex:migrate:up": "knex migrate:up --knexfile src/utils/knexfile.ts",
        "knex:migrate:down": "knex migrate:down --knexfile src/utils/knexfile.ts",
        "knex:migrate:latest": "knex migrate:latest --knexfile src/utils/knexfile.ts",
        "knex:migrate:rollback": "knex migrate:rollback --knexfile src/utils/knexfile.ts",
        "knex:migrate:rollback:all": "knex migrate:rollback --all --knexfile src/utils/knexfile.ts",
        "knex:seed:make": "knex seed:make --knexfile src/utils/knexfile.ts",
        "knex:seed:run": "knex seed:run --knexfile src/utils/knexfile.ts",
        "preversion": "yarn dist && git add --all && git commit -m \"Build\"",
        "postversion": "git push --tags",
        "release:patch": "yarn version --patch",
        "release:minor": "yarn version --minor",
        "release:major": "yarn version --major",
        "release:prerelease": "yarn version --prerelease"
    },
    "bin": {
        "via-profit-core": "./dist/bin/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git@gitlab.com:via-profit-services/core.git"
    },
    "author": "Via Profit <promo@via-profit.ru>",
    "license": "MIT",
